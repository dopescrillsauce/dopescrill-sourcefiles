
/* ------- please2 style sheet ------------------------------------------- */



/* ipad TRAVEL ------------------------------------------- */



#slider-travel {
	text-align: center;
}


label, a {
	color: teal;
	cursor: pointer;
	text-decoration: none;
}
label:hover, a:hover {
	color: #000 !important;
}
* { -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box; }

label, #active-travel, img { -moz-user-select:none;-webkit-user-select:none; }

.catch { display: block; height: 0; overflow: hidden; }

#slider-travel {
	margin: 0 auto;
	max-width:840px;
padding-bottom:100px;
}


/* NEW EXPERIMENT */
/* Slider Setup */

input {
	display: none;
}

#slide1:checked ~ #slides-travel .inner-travel { margin-left:0; }
#slide2:checked ~ #slides-travel .inner-travel { margin-left:-100%; }
#slide3:checked ~ #slides-travel .inner-travel { margin-left:-200%; }
#slide4:checked ~ #slides-travel .inner-travel { margin-left:-300%; }
#slide5:checked ~ #slides-travel .inner-travel { margin-left:-400%; }

#overflow-travel {
	width: 100%;
	overflow: hidden;
}

article img {
	width: 100%;
}

#slides-travel .inner-travel {
	width: 500%;
	line-height: 0;
}

#slides-travel article {
	width: 20%;
	float: left;
}

/* Slider Styling */

/* Control Setup */

#controls-travel {
	margin: 0 0 0 25%;
	width: 50%;
	height: 60px;

}

#controls-travel label { 
	display: none;
	width: 20px;
	height: 20px;
	opacity: 0.7;

}

#active-travel {
	margin: 10px 0 0;
	text-align: center;
	display:none;
}

#active-travel label {
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	display: inline-block;
	width: 10px;
	height: 10px;
	background: #bbb;
}

#active-travel label:hover {
	background: #ccc;
	border-color: #777 !important;
}

#controls-travel label:hover {
	opacity: 1;

}

#slide1:checked ~ #controls-travel label:nth-child(2), 
#slide2:checked ~ #controls-travel label:nth-child(3), 
#slide3:checked ~ #controls-travel label:nth-child(4), 
#slide4:checked ~ #controls-travel label:nth-child(5), 
#slide5:checked ~ #controls-travel label:nth-child(1) {
	background: url('http://www.phlume.com/chad/testtest/next.png') no-repeat;
	float: right;
	margin: 0 10px 0 0;
	display: block;

}


#slide1:checked ~ #controls-travel label:nth-child(5),
#slide2:checked ~ #controls-travel label:nth-child(1),
#slide3:checked ~ #controls-travel label:nth-child(2),
#slide4:checked ~ #controls-travel label:nth-child(3),
#slide5:checked ~ #controls-travel label:nth-child(4) {
	background: url('http://www.phlume.com/chad/testtest/prev.png') no-repeat;
	float: left;
	margin: 0 0 0 10px;
	display: block;
}

#slide1:checked ~ #active-travel label:nth-child(1),
#slide2:checked ~ #active-travel label:nth-child(2),
#slide3:checked ~ #active-travel label:nth-child(3),
#slide4:checked ~ #active-travel label:nth-child(4),
#slide5:checked ~ #active-travel label:nth-child(5) {
	background: #333;
	border-color: #333 !important;
}



/* Slider Styling */

#slides-travel {
	margin: 0px 0 0;
	x-webkit-border-radius: 5px;
	x-moz-border-radius: 5px;
	xborder-radius: 5px;
	xbox-shadow: 1px 1px 4px #666;
	padding: 1%;
	background: ;
}


/* Animation */

#slides-travel .inner-travel {
	-webkit-transform: translateZ(0);
	-webkit-transition: all 800ms cubic-bezier(0.770, 0.000, 0.175, 1.000); 
	-moz-transition: all 800ms cubic-bezier(0.770, 0.000, 0.175, 1.000); 
    -ms-transition: all 800ms cubic-bezier(0.770, 0.000, 0.175, 1.000); 
     -o-transition: all 800ms cubic-bezier(0.770, 0.000, 0.175, 1.000); 
        transition: all 800ms cubic-bezier(0.770, 0.000, 0.175, 1.000); /* easeInOutQuart */

	-webkit-transition-timing-function: cubic-bezier(0.770, 0.000, 0.175, 1.000); 
	-moz-transition-timing-function: cubic-bezier(0.770, 0.000, 0.175, 1.000); 
    -ms-transition-timing-function: cubic-bezier(0.770, 0.000, 0.175, 1.000); 
     -o-transition-timing-function: cubic-bezier(0.770, 0.000, 0.175, 1.000); 
        transition-timing-function: cubic-bezier(0.770, 0.000, 0.175, 1.000); /* easeInOutQuart */
}

#slider-travel {
	-webkit-transform: translateZ(0);
	-webkit-transition: all 0.5s ease-out;
	-moz-transition: all 0.5s ease-out;
	-o-transition: all 0.5s ease-out;
	transition: all 0.5s ease-out;
}

#controls-travel label{
	-webkit-transform: translateZ(0);
	-webkit-transition: opacity 0.2s ease-out;
	-moz-transition: opacity 0.2s ease-out;
	-o-transition: opacity 0.2s ease-out;
	transition: opacity 0.2s ease-out;
}

#slide1:checked ~ #slides-travel article:nth-child(1) .info,
#slide2:checked ~ #slides-travel article:nth-child(2) .info,
#slide3:checked ~ #slides-travel article:nth-child(3) .info,
#slide4:checked ~ #slides-travel article:nth-child(4) .info,
#slide5:checked ~ #slides-travel article:nth-child(5) .info {
	opacity: 1;
	-webkit-transition: all 1s ease-out 0.6s;
	-moz-transition: all 1s ease-out 0.6s;
	-o-transition: all 1s ease-out 0.6s;
	transition: all 1s ease-out 0.6s;
}

.info, #controls-travel, #slides-travel, #active-travel, #active-travel label, .info h3, .desktop, .tablet, .mobile {
	-webkit-transform: translateZ(0);
	-webkit-transition: all 0.5s ease-out;
	-moz-transition: all 0.5s ease-out;
	-o-transition: all 0.5s ease-out;
	transition: all 0.5s ease-out;
}


@media only screen and (max-width: 850px) and (min-width: 450px) {

